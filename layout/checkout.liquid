  <body>
    {{ skip_to_content_link }}
    
    {%- capture header_logos -%}
      <div class="header-logos">
        <div class="header-logos--logo">
          {{ content_for_logo | replace: 'href', 'data-href' }}
        </div>
        {%- if settings.checkout_trust_logo != blank -%}
          <div class="header-logos--trust">
            <img
              loading="lazy"
              src="{{ settings.checkout_trust_logo | img_url: 'master' }}"
              alt="Trust image">
          </div>
        {%- endif -%}
      </div>
    {%- endcapture -%}
      
    <header class="banner" data-header role="banner">
      <div class="wrap">
        {{ header_logos }}
      </div>
    </header>
    
    {{ order_summary_toggle }}
    <div class="content" data-content>
      <div class="wrap">
        <div class="main">
          <header class="main__header" role="banner">
            {{ header_logos }}
            {{ breadcrumb }}
            <div class="express-checkout">
              {{ alternative_payment_methods }}
              {%- if settings.xtra_pay_heading != blank -%}                
                <div class="express-checkout--extra">
                  {%- if settings.xtra_pay_heading != blank -%}
                    <div class="express-checkout--extra-heading">
                      {{ settings.xtra_pay_heading }}
                    </div>
                  {%- endif -%}
                  {%- if settings.xtra_pay_icons != blank -%}
                    <div class="express-checkout--extra-icons">
                      <img
                        loading="lazy"
                        src="{{ settings.xtra_pay_icons | img_url: 'master' }}"
                        alt="{{ settings.xtra_pay_icons.alt }}">
                    </div>
                  {%- endif -%}
                </div>
              {%- endif -%}
            </div>
          </header>
          <main class="main__content" role="main">
            {{ content_for_layout }}
          </main>
          <footer class="main__footer" role="contentinfo">
            
            <div class="sidebar-supbox hidden-laptop">
              {% section 'checkout-support-box' %}
            </div>

            <div class="checkout-footer__text">{{ settings.checkout_text }}</div>

            <div class="checkout-page--payicons">
              <ul class="inline-list payment-icons">
                {%- for type in shop.enabled_payment_types -%}
                  <li class="icon--payment">
                    {{ type | payment_type_svg_tag }}
                  </li>
                {%- endfor -%}
              </ul>
            </div>
            
            {{ content_for_footer }}
            
          </footer>
        </div>
        <aside class="sidebar" role="complementary">
          <div class="sidebar__header">
            {{ content_for_logo }}
          </div>
          <div class="sidebar__content">
            {{ content_for_order_summary }}
            
            <div class="sidebar-supbox hidden-pocket">
              {% section 'checkout-support-box' %}
            </div>
          </div>
        </aside>
      </div>
    </div>
    
    {{ tracking_code }}

    <script>
      document.querySelector('.breadcrumb').innerHTML += `<li class="breadcrumb__item">
          <a class="breadcrumb__text">Fertig</a>
        </li>`;
      document.querySelectorAll('.breadcrumb .breadcrumb__item.breadcrumb__item--completed').forEach(elem => {
        elem.addEventListener("click", (e) => {
          elem.querySelector(".breadcrumb__link").click();
        });
      });
    </script>
    {%- render 'checkout-street-validation' -%}
  </body>
